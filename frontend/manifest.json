{
    "name": "Phishing Detection",
    "description": "Phishing Detection!",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
      },
    "permissions": [
        "storage", 
        "activeTab", 
        "scripting",
        "http://127.0.0.1:8000/"
    ],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "128": "/images/fish.png"
        }
    },
    "icons": {
        "128": "/images/fish.png"
    },
    "options_page": "options.html"
}